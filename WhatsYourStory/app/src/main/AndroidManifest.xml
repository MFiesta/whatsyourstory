<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.si543.whatsyourstory" >

<uses-sdk android:minSdkVersion="8" android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name="com.example.si543.whatsyourstory.LogInActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.si543.whatsyourstory.FeedActivity"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="com.example.si543.whatsyourstory.SignUpActivity"
            android:label="Log In"
            android:parentActivityName="com.example.si543.whatsyourstory.LogInActivity">
            <!-- A child of the main activity -->
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.si543.whatsyourstory.LogInActivity" />
        </activity>

        <activity
            android:name="com.example.si543.whatsyourstory.SetMyInitProfileActivity"
            android:label="Sign Up"
            android:parentActivityName="com.example.si543.whatsyourstory.SignUpActivity">
            <!-- A child of the main activity -->
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.si543.whatsyourstory.SignUpActivity" />
        </activity>

        <activity

            android:name="com.example.si543.whatsyourstory.ChooseSkillsActivity"
            android:label="Profile setting"
            android:parentActivityName="com.example.si543.whatsyourstory.SetMyInitProfileActivity">
            <!-- A child of the main activity -->
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.si543.whatsyourstory.SetMyInitProfileActivity" />

        </activity>

        <activity
            android:name="com.example.si543.whatsyourstory.ChooseInterestsActivity"
            android:label="Skills"
            android:parentActivityName="com.example.si543.whatsyourstory.ChooseSkillsActivity">
            <!-- A child of the main activity -->
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.si543.whatsyourstory.ChooseSkillsActivity" />
        </activity>

        <activity
            android:name="com.example.si543.whatsyourstory.OtherUserProfileActivity"
            android:label="WhatsYourStory"
            android:parentActivityName="com.example.si543.whatsyourstory.FeedActivity">
            <!-- A child of the main activity -->
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.si543.whatsyourstory.FeedActivity" />
        </activity>

        <activity
            android:name="com.example.si543.whatsyourstory.FavoritesActivity"
            android:label="Eytan Adar"
            android:parentActivityName="com.example.si543.whatsyourstory.OtherUserProfileActivity">
            <!-- A child of the main activity -->
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.si543.whatsyourstory.FavoritesActivity" />
        </activity>

    </application>

</manifest>
